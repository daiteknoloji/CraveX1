.mx_MobileMenuButton {
    display: none;

    /* Show only on mobile */
    @media only screen and (max-width: 768px) {
        display: flex !important;
        min-width: 44px;
        min-height: 44px;
        padding: 10px;
    }

    svg {
        width: 24px;
        height: 24px;
    }
}

